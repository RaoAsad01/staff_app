{
  "expo": {
    "name": "staffapp",
    "slug": "staffapp",
    "jsEngine": "hermes",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "splash": {
  "backgroundColor": "#000000"
},
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asadahmad.staffapp",
      "newArchEnabled": true
    },
   "android": {
  "package": "com.asad_ahmad.staffapp",
  "permissions": [
    "CAMERA",
    "RECORD_AUDIO"
  ],
  "minSdkVersion": 23,
  "compileSdkVersion": 35,
  "targetSdkVersion": 35,
  "enableProguardInReleaseBuilds": true,
  "enableShrinkResources": true,
  "newArchEnabled": true
},
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f2a44f35-43a5-4a2e-b281-7eba229ea443"
      }
    }
  }
}
